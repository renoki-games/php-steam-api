{
    "name": "renoki-games/php-steam-api",
    "description": "PHP SDK implementation for Valve's Steam API.",
    "keywords": ["laravel", "php", "steam", "valve", "sdk", "wrapper", "api"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-games/php-steam-api",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiGames\\Steam\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiGames\\Steam\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiGames\\Steam\\SteamServiceProvider"
            ]
        }
    }
}
